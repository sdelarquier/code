\par \tab logical*1 bad(2,0:15,ng),fgood(0:15,0:15),fvlos(2,0:15,0:15)
\par \tab logical*1 flag_data,flag_change
\par \tab character*40 rcs_rev
\par \tab common/radars/pos
\par \tab common/hght/v_height
\par \tab common/gate/range
\par \tab common/date/sdate,edate
\par \tab common/param/mhz,khz,lagfr,smsep,intt,nrang
\par \tab common/old_parm/old_lag,old_sep
\par \tab common/vradial/vlos,fvlos
\par \tab common/table/latg,long,alt,latm,lonm,az,abb,n,rng,bx,by,bz,kx,ky,kz
\par \tab common/data/p,v,verr_v,verr_w,width,werr,el,el_low,el_high,bad,gsc,n_avg
\par \tab data unit_dat/3/, unit_prt/4/
\par c_______________________________
\par 
\par 
\par \tab open(unit_dat,file='merge.dat',status='unknown',form='unformatted')
\par \tab open(unit_prt,file='merge.prt',status='unknown',form='formatted')
\par 
\par \tab rcs_rev = '$Revision: 2.1 $'
\par \tab print 88, rcs_rev
\par \tab write(unit_prt,88) rcs_rev
\par  88\tab format(//,' MERGE: ',a40,//)
\par 
\par \tab call MERGE_INFO
\par \tab \tab \tab \tab \tab \tab !writes the general
\par \tab write(unit_dat) rcs_rev,infi,infr\tab \tab !header on output file
\par c\tab -------------------
\par 
\par c... Makes the first read of data to initialize parameters ...
\par 
\par \tab map = 0
\par \tab st = CNV_MDHMS_SEC(syr,smo,sday,shr,smin,ssec)
\par \tab et = CNV_MDHMS_SEC(eyr,emo,eday,ehr,emin,esec)
\par 
\par \tab call RADAR_POS(st1,syr,st,pos1)
\par \tab call RADAR_POS(st2,syr,st,pos2)
\par \tab do j=1,5
\par \tab   pos(1,j) = pos1(j)
\par \tab   pos(2,j) = pos2(j)
\par \tab enddo
\par 
\par \tab t = st
\par    2\tab call READ_INIT(t)
\par 
\par \tab write(unit_prt,303) st1,lagfr(1),smsep(1),intt(1),nrang(1)
\par \tab write(unit_prt,304) st2,lagfr(2),smsep(2),intt(2),nrang(2)
\par  303\tab format(/,' Radar1 ident = ',i2,',  lagfr,smsep,intt,nrang ',4i6)
\par  304\tab format(  ' Radar2 ident = ',i2,',  lagfr,smsep,intt,nrang ',4i6)
\par 
\par c... Makes (or read) the lookup table if elev. angle
\par c... information has not to be used (ialt=1 or 2) ...
\par 
\par \tab if(ialt.ne.3) then
\par c\tab   call LOOK_TABLE(ier)
\par c\tab   if(ier.eq.1) then
\par \tab     call VIRTUAL
\par \tab     call MAKE_TABLE
\par c\tab   endif
\par \tab endif
\par 
\par c************************************
\par c... Starts Data Processing ...
\par c************************************
\par 
\par   1\tab map = map + 1
\par \tab write(unit_prt,200) map
\par  200\tab format(/,'Map ',i3)
\par 
\par \tab call READ_SCAN(t,flag_data,flag_change,time_change)
\par 
\par \tab if(flag_data) then
\par 
\par \tab   if(ialt.eq.3) then\tab \tab \tab !compute lookup table
\par \tab     call VIRTUAL\tab \tab \tab !parameters for each
\par \tab     call MAKE_TABLE
\par \tab   endif\tab \tab \tab \tab \tab !angle is used
\par 
\par \tab   call MERGE_VEC(ngood,fgood,vx,vy,vz,vx_erv,vy_erv,vz_erv,vx_erw,vy_erw,vz_erw,pnt_qlty)
\par 
\par \tab   if(idiv.eq.1) then
\par \tab     call HOR_PROJ(+1,vx,vy,vz,bx,by,bz)
\par \tab     call DIV_FREE(vx,vy,latg,long,az,fgood,niter)
\par \tab     if(iext.eq.1) call DIV_EXTEND(vx,vy,latg,long,az,fgood,ngood)
\par \tab     call HOR_PROJ(-1,vx,vy,vz,bx,by,bz)
\par \tab   endif
\par 
\par \tab \tab \tab \tab \tab \tab !quality index for the map
\par \tab   map_qlty = QUALITY_MAP(ngood,fgood,pnt_qlty)
\par 
\par \tab endif
\par 
\par c... Writes header for the scan on output file ...
\par 
\par \tab write(unit_dat) sdate,edate,mhz,khz,lagfr,smsep,map_qlty,ngood,niter,height_r
\par c\tab ---------------
\par 
\par \tab write(unit_prt,7040) map_qlty
\par \tab write(unit_prt,7050) ngood
\par \tab write(unit_prt,7060) niter
\par 
\par  7040\tab format('  Map quality:\tab \tab ',i8)
\par  7050\tab format('  Number of good points:\tab ',i8)
\par  7060\tab format('  Number of iterations:\tab ',i8)
\par 
\par c... Writes data to output file ...
\par 
\par \tab if(ngood.ne.0) then
\par \tab   do ib=0,15
\par \tab     do jb=0,15
\par \tab       if(fgood(ib,jb)) then
\par \tab         write(unit_dat) ib,jb,
\par c\tab         -------------------
\par      &\tab \tab \tab  latg(ib,jb),long(ib,jb),
\par      &\tab \tab \tab  latm(ib,jb),lonm(ib,jb),
\par      &\tab \tab \tab  bx(ib,jb),by(ib,jb),bz(ib,jb),
\par      &\tab \tab \tab  vlos(1,ib,jb),vlos(2,ib,jb),
\par      &\tab \tab \tab  vx(ib,jb),vy(ib,jb),vz(ib,jb),
\par      &\tab \tab \tab  vx_erv(ib,jb),vy_erv(ib,jb),vz_erv(ib,jb),
\par      &\tab \tab \tab  vx_erw(ib,jb),vy_erw(ib,jb),vz_erw(ib,jb),
\par      &\tab \tab \tab  pnt_qlty(ib,jb)
\par 
\par c\tab \tab write(unit_prt,8000) ib,jb
\par c\tab \tab write(unit_prt,8010) latg(ib,jb), long(ib,jb)
\par c\tab \tab write(unit_prt,8020) latm(ib,jb), lonm(ib,jb)
\par c\tab \tab write(unit_prt,8030) vlos(1,ib,jb), vlos(2,ib,jb)
\par c\tab \tab write(unit_prt,8040) bx(ib,jb), by(ib,jb), bz(ib,jb)
\par c\tab \tab write(unit_prt,8050) vx(ib,jb), vy(ib,jb), vz(ib,jb)
\par c\tab \tab write(unit_prt,8060) vx_erv(ib,jb),vy_erv(ib,jb),vz_erv(ib,jb)
\par c\tab \tab write(unit_prt,8070) vx_erw(ib,jb),vy_erw(ib,jb),vz_erw(ib,jb),pnt_qlty(ib,jb)
\par 
\par  8000\tab \tab format('========== Indices i, j:',2i13,' ==========')
\par  8010\tab \tab format('GG Latitude and Longitude:',2f14.4)
\par  8020\tab \tab format('MG Latitude and Longitude:',2f14.4)
\par  8030\tab \tab format('vlos12 :\tab \tab   ',2f14.3)
\par  8040\tab \tab format('Bxyz :\tab \tab \tab   ',3f14.7)
\par  8050\tab \tab format('Vx Vy Vz :\tab \tab   ',3f14.3)
\par  8060\tab \tab format('EVxyz :\tab \tab \tab   ',3f14.5)
\par  8070\tab \tab format('EWxyz and qlty :\tab   ',3f14.5,i8)
\par \tab       endif
\par \tab     enddo
\par \tab   enddo
\par \tab endif
\par 
\par \tab \tab \tab \tab !start time of next scan
\par \tab if(.not.flag_change) then
\par \tab   t = t + tperiod
\par \tab else
\par \tab   t = time_change
\par \tab   go to 2
\par \tab endif
\par 
\par \tab if(t.lt.et) go to 1\tab !process next scan
\par 
\par \tab stop
\par \tab end
\par \pard\plain \s253\qc\fi284\sb360\sa360 \b\f3\lang1036 \page ANNEX 2 : Subroutine MERGE_INFO
\par \pard\plain \fi284\sl-240 \fs20\lang1036 \tab subroutine MERGE_INFO
\par c\tab *********************
\par c
\par c**********************************************************************
\par c This routine sets up the different options of the merge process
\par c These options are kept in the inclusion 'info.inc'
\par c
\par c\tab $Log:\tab merge_info.f,v $
\par c Revision 1.1  94/09/01  16:13:51  16:13:51  senior (Catherine SENIOR)
\par c Initial revision
\par c 
\par c\tab $Revision: 1.1 $
\par c**********************************************************************
\par 
\par \tab include \tab \tab 'info.inc'
\par \tab include\tab \tab 'radops.par'
\par \tab include\tab  \tab 'raddat386_1_3.inc'
\par \tab equivalence\tab (parms,parm_list)
\par \tab integer*4 \tab st,et,CNV_MDHMS_SEC
\par \tab integer*4\tab FITROPEN, READ_DATREC, FIT_CLOSE
\par \tab integer*4 \tab iostat
\par \tab integer*2 \tab kst
\par \tab character*1  \tab ans
\par \tab character*1\tab NUL, TAB
\par \tab character*20\tab r, RADAR_NAME
\par \tab character*50 \tab fidat,fidat1,fidat2
\par \tab character*50\tab fname1, fname2
\par \tab logical*1\tab afname1(50), afname2(50)
\par \tab logical*1\tab bnul /0/
\par \tab logical*1\tab btab /9/
\par \tab equivalence\tab (afname1,fname1), (afname2,fname2)
\par \tab equivalence\tab (NUL,bnul), (TAB, btab)
\par c___________________________
\par 
\par 
\par c... Opens data files ...
\par 
\par  10\tab print *,' Enter Radar1 file name (with no extension)'
\par \tab read '(a)', fidat1
\par \tab do i=50,1,-1
\par \tab   if(fidat1(i:i) .eq. ' ') fidat1(i:i) = NUL
\par \tab   if(fidat1(i:i) .eq. TAB) fidat1(i:i) = NUL
\par \tab enddo
\par \tab fname1 = fidat1
\par \tab ifileptr(1) = FITROPEN(afname1)
\par \tab if(ifileptr(1) .le. 0) then
\par \tab   print *,' This file does not exist '
\par \tab   print *,' Try again '
\par \tab   go to 10
\par \tab endif
\par \tab iostat = 1
\par \tab rec_time = 0
\par \tab iostat = READ_DATREC(ifileptr(1))
\par \tab if(iostat .ne. 1) print *,' iostat = ', iostat
\par \tab st1 = parms386.st_id
\par 
\par  20 \tab print *,' Enter Radar2 file name (with no extension)'
\par \tab read '(a)', fidat2
\par \tab do i=50,1,-1
\par \tab   if(fidat2(i:i) .eq. ' ') fidat2(i:i) = NUL
\par \tab   if(fidat2(i:i) .eq. TAB) fidat2(i:i) = NUL
\par \tab enddo
\par \tab fname2 = fidat2
\par \tab ifileptr(2) = FITROPEN(afname2)
\par \tab if(ifileptr(2) .le. 0) then
\par \tab   print *,' This file does not exist '
\par \tab   print *,' Try again '
\par \tab   go to 20
\par \tab endif
\par \tab iostat = 1
\par \tab rec_time = 0
\par \tab iostat = READ_DATREC(ifileptr(2))
\par \tab if(iostat .ne. 1) print *,' iostat = ', iostat
\par \tab st2 = parms386.st_id
\par 
\par \tab call PAIR(st1,st2,ier)\tab !Checks that radars are compatible
\par 
\par \tab if(ier.eq.1) then\tab !Radar1 should be west of Radar2
\par \tab   print *,' Radar1 should be west of Radar2 '
\par \tab   print *,' Files will be closed, exchanged, and re-openned '
\par \tab   print *,' '
\par \tab   do k=1,2
\par \tab     iostat = FIT_CLOSE(%val(ifileptr(k)))
\par \tab   enddo
\par \tab   kst = st1\tab \tab !Exchanges station IDs
\par \tab   st1 = st2
\par \tab   st2 = kst
\par \tab   fidat  = fidat1\tab !Exchanges files
\par \tab   fidat1 = fidat2
\par \tab   fidat2 = fidat
\par \tab   fname1 = fidat1
\par \tab   fname2 = fidat2
\par \tab   ifileptr(1) = FITROPEN(afname1)
\par \tab   ifileptr(2) = FITROPEN(afname2)
\par \tab else if(ier.eq.2) then
\par \tab   print *,' '
\par \tab   print *,' This is not a nominal pair of radars, but '
\par \tab   print *,' the fields of view overlap '
\par \tab   print *,' Do you want to continue?'
\par \tab   read '(a)', ans
\par \tab   if(ans.eq.'n'.or.ans.eq.'N') stop
\par \tab   print *,' '
\par \tab else if(ier.eq.3) then
\par \tab   print *,' '
\par \tab   print *,' Radars are not in the same hemisphere '
\par \tab   print *,' Try again'
\par \tab   print *,' '
\par \tab   go to 10
\par \tab else if(ier.eq.4) then
\par \tab   print *,' '
\par \tab   print *,' Incompatible radar pair: the fields of view '
\par \tab   print *,' do not overlap. Try again'
\par \tab   print *,' '
\par \tab   go to 10
\par \tab endif
\par 
\par c... Choice of start and end time ...
\par 
\par \tab print *,' '
\par \tab print *,' Enter year (yy)'
\par \tab read *, syr
\par \tab eyr = syr
\par    2\tab print *,' Enter start time (mm,id,ih,mn,is)'
\par \tab read *, smo,sday,shr,smin,ssec
\par \tab print *, 'Enter  end  time (mm,id,ih,mn,is)'
\par \tab read *, emo,eday,ehr,emin,esec
\par \tab st = CNV_MDHMS_SEC(syr,smo,sday,shr,smin,ssec)
\par \tab et = CNV_MDHMS_SEC(eyr,emo,eday,ehr,emin,esec)
\par \tab if(et.lt.st) then
\par \tab   print *,' end time should be larger than start time '
\par \tab   go to 2
\par \tab endif
\par 
\par c... Averaged or individual maps ...
\par 
\par \tab print *,' '
\par \tab print *,' Averaging maps: Options...'
\par \tab print *,'\tab 1 : map for each scan '
\par \tab print *,'\tab 2 : average maps '
\par    3\tab print *,' Enter choice'
\par   \tab read *, idur
\par \tab if(idur.ne.1.and.idur.ne.2) go to 3
\par \tab tperiod = 0.
\par \tab if(idur.eq.2) then
\par \tab   print *,' Enter averaging time (sec) '
\par \tab   read *, tperiod 
\par \tab endif
\par 
\par c... Choice of extreme frequencies for merging data from the two radars ...
\par 
\par \tab do i=1,2
\par \tab   f_low(i) = 8
\par \tab   f_high(i) = 20
\par \tab enddo
\par \tab print *,' '
\par \tab print *,' Extreme frequencies are 8 and 20 MHz...'
\par \tab print *,' Do you want to change them? (y or n)'
\par \tab read '(a)', ans
\par \tab if(ans.eq.'y'.or.ans.eq.'Y') then
\par \tab   print *,' Enter radar1 extreme frequencies...'
\par \tab   print *,'\tab low limit \tab '
\par \tab   read *, f_low(1)
\par \tab   print *,'\tab high limit \tab '
\par \tab   read *, f_high(1)
\par \tab   print *,' Enter radar2 extreme frequencies...'
\par \tab   print *,'\tab low limit \tab '
\par \tab   read *, f_low(2)
\par \tab   print *,'\tab high limit \tab '
\par \tab   read *, f_high(2)
\par \tab endif
\par 
\par c... Altitude of virtual height and method for setting it ...
\par 
\par \tab ialt = 1
\par \tab print *,' '
\par    4\tab print *,' Setting the virtual height: Options... '
\par \tab print *,'\tab 1 : Constant virtual height '
\par \tab print *,'\tab 2 : Model for virtual height '
\par \tab print *,'\tab 3 : Use elevation angle information '
\par \tab print *,' Enter choice '
\par \tab read *, ialt
\par \tab if(ialt.eq.2.or.ialt.eq.3) then
\par \tab   print *,' Option not ready '
\par \tab   print *,' Constant virtual height will be assumed'
\par \tab   ialt = 1
\par \tab endif
\par \tab height = 400.
\par \tab if(ialt.eq.1) then
\par \tab   print *,' Altitude of virtual height is 400. km...'
\par \tab   print *,' Do you want to change it? (y or n) '
\par \tab   read '(a)',ans
\par \tab   if(ans.eq.'y'.or.ans.eq.'Y') then
\par \tab     print *,' Altitude of virtual height ? '
\par \tab     read *, height
\par \tab   endif
\par \tab endif
\par 
\par \tab \tab \tab \tab \tab !real stnd height
\par \tab height_r = 100. + (height-100.) * 0.75
\par 
\par c... Processing of ground scatter ...
\par 
\par \tab print *,' '
\par    5\tab print *,' Processing ground scatter echoes: Options...'
\par \tab print *,'\tab 1 : Include all echoes identified'
\par \tab print *,'\tab     as ground scatter by FITACF '
\par \tab print *,'\tab 2 : Exclude all echoes identified'
\par \tab print *,'\tab     as ground scatter by FITACF '
\par \tab print *,'\tab 3 : Include echoes if identified as ground'
\par \tab print *,'\tab     scatter by FITACF for one of the radars'
\par \tab print *,'\tab     but not for the other one '
\par \tab print *,'\tab 4 : Other (to be defined) '
\par \tab print *,' Enter choice '
\par \tab read *, igs
\par \tab if(igs.ge.3) then
\par \tab   print *,' Option not ready '
\par \tab   go to 5
\par \tab endif
\par 
\par c... E-region echoes ...
\par 
\par \tab print *,' '
\par    6\tab print *,' Processing E-region echoes: Options... '
\par \tab print *,'\tab 1 : Include E-region echoes '
\par \tab print *,'\tab 2 : Exclude E-region echoes by setting a'
\par \tab print *,'\tab     minimum range for F-region echoes'
\par \tab print *,'\tab 3 : Exclude E-region echoes by using the'
\par \tab print *,'\tab     elevation angle information'
\par \tab print *,' Enter choice '
\par \tab read *, iereg
\par \tab frng_min = 900.
\par \tab if(iereg.eq.2) then
\par \tab   print *,' Minimum range for F-region echoes is 900 km...'
\par \tab   print *,' Do you want to change it? (y or n) '
\par \tab   read '(a)', ans
\par \tab   if(ans.eq.'y'.or.ans.eq.'Y') then
\par \tab     print *,' Enter minimum range for F-region echoes '
\par \tab     read *, frng_min
\par \tab   endif
\par \tab else if(iereg.eq.3) then
\par \tab   print *,' Option not ready '
\par \tab   go to 6
\par \tab endif
\par 
\par c... Processing choices ...
\par 
\par \tab ifltr = 0
\par \tab print *,' '
\par \tab print *,' Radial velocity smoothing ? [N] '
\par \tab read '(a)', ans
\par \tab if(ans.eq.'y'.or.ans.eq.'Y') ifltr = 1
\par 
\par \tab inter = 0
\par c\tab print *,' '
\par c\tab print *,' Interpolation of radial velocities to '
\par c\tab print *,' isolated missing points ? [N]'
\par c\tab read '(a)', ans
\par c\tab if(ans.eq.'y'.or.ans.eq.'Y') inter = 1
\par 
\par \tab idiv = 0
\par \tab print *,' Divfree analysis for relaxation? [N] '
\par \tab read '(a)', ans
\par \tab if(ans.eq.'y'.or.ans.eq.'Y') then
\par \tab   idiv = 1
\par \tab   if( (st2.eq.11) .or. (st2.eq.12) ) then
\par \tab     print *,' The present divergence-free method is not valid'
\par \tab     print *,' for these radars because the geographic pole is'
\par \tab     print *,' within the field of view of the radars.'
\par \tab     print *,' The divergence-free analysis will not be done.'
\par \tab     idiv = 0
\par \tab   endif
\par \tab endif
\par 
\par \tab iext = 0
\par \tab if(idiv.eq.1) then
\par \tab   print *,' Divfree analysis for extension of the map ? [N] '
\par \tab   read '(a)', ans
\par \tab   if(ans.eq.'y'.or.ans.eq.'Y') iext = 1 
\par \tab endif
\par 
\par \tab ires = 0
\par c  7\tab print *,' Increase spatial resolution ? [N] '
\par c\tab read '(a)', ans
\par c\tab if(ans.eq.'y'.or.ans.eq.'Y') ires = 1
\par 
\par c... Limit values for parameters ...
\par 
\par \tab pwr_min = 3.
\par \tab vel_min = 0.
\par \tab vel_max = 3000.
\par \tab velerr_max = 50.
\par 
\par \tab print *,' '
\par \tab print *,' Minimum power is 3. db...'
\par \tab print *,' Do you want to change it? (y or n)'
\par \tab read '(a)', ans
\par \tab if(ans.eq.'y'.or.ans.eq.'Y') then
\par \tab   print *,' Minimum power ? '
\par \tab   read *, pwr_min
\par \tab endif
\par 
\par \tab print *,' Minimum velocity is 0. m/s...'
\par \tab print *,' Do you want to change it? (y or n)'
\par \tab read '(a)', ans
\par \tab if(ans.eq.'y'.or.ans.eq.'Y') then
\par \tab   print *,' Minimum velocity ? '
\par \tab   read *, vel_min
\par \tab endif
\par 
\par \tab print *,' Maximum velocity is 3000. m/s...'
\par \tab print *,' Do you want to change it? (y or n)'
\par \tab read '(a)', ans
\par \tab if(ans.eq.'y'.or.ans.eq.'Y') then
\par \tab   print *,' Maximum velocity ? '
\par \tab   read *, vel_max
\par \tab endif
\par 
\par \tab print *,' Maximum error on velocity is 50. m/s...'
\par \tab print *,' Do you want to change it? (y or n)'
\par \tab read '(a)', ans
\par \tab if(ans.eq.'y'.or.ans.eq.'Y') then
\par \tab   print *,' Maximum error velocity ? '
\par \tab   read *, velerr_max
\par \tab endif
\par 
\par \tab print *,' '
\par \tab r = RADAR_NAME(st1)
\par \tab print '(a)', 'Radar1: ', r
\par \tab r = RADAR_NAME(st2)
\par \tab print '(a)', 'Radar2: ', r
\par \tab print *,' '
\par 
\par \tab return
\par \tab end
\par \pard\plain \s253\qc\fi284 \b\f3\lang1036 \page ANNEX 3 : File INFO.INC
\par \pard\plain \fi284\sb120 \fs20\lang1036 
\par \pard \fi284 {\f3 c\tab INFO.INC
\par }{\f3 c\tab ********
\par }{\f3 
\par }{\f3 c*********************************************************************
\par }{\f3 c \tab \tab \tab Meaning of few variables:
\par }{\f3 c\tab \tab \tab -------------------------
\par }{\f3 c
\par }{\f3 c\tab idur = 1:  (*)\tab Map for each scan
\par }{\f3 c\tab    }{\f3      = 2:\tab Average scans (during time 'tperiod')
\par }{\f3 c
\par }{\f3 c\tab ialt = 1:\tab \tab Constant virtual height
